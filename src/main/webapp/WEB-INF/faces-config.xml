<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
                                  http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
  <navigation-rule>
      <from-view-id>/index.xhtml</from-view-id> <!-- From /index.xhtml -->
      <navigation-case>
          <from-outcome>applicationPage</from-outcome> <!-- When outcome == applicationPage -->
          <to-view-id>/views/application.xhtml</to-view-id> <!-- goto /views/application.xhtml -->
      </navigation-case>
  </navigation-rule>
    <navigation-rule>
        <from-view-id>*</from-view-id> <!-- From any -->
        <navigation-case>
            <from-outcome>startPage</from-outcome> <!-- When outcome == startPage -->
            <to-view-id>/index.xhtml</to-view-id> <!-- goto /index.xhtml -->
        </navigation-case>
    </navigation-rule>


</faces-config>